{
  "version": 1,
  "projects": {
    "octra": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/octra",
      "sourceRoot": "apps/octra/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/octra",
            "index": "apps/octra/src/index.html",
            "main": "apps/octra/src/main.ts",
            "polyfills": "apps/octra/src/polyfills.ts",
            "tsConfig": "apps/octra/tsconfig.app.json",
            "allowedCommonJsDependencies": [
              "js-sha256",
              "x2js",
              "konva",
              "crossevent",
              "contra/emitter",
              "flat",
              "dragula",
              "jquery",
              "platform",
              "global/document",
              "videojs",
              "ajv",
              "querystring",
              "crossvent",
              "xmldom",
              "videojs-vtt.js",
              "@videojs/xhr",
              "@babel/runtime/helpers/possibleConstructorReturn",
              "@babel/runtime/helpers/construct",
              "@babel/runtime/helpers/inherits",
              "mux.js/lib/tools/mp4-inspector",
              "mux.js/lib/tools/ts-inspector.js",
              "mux.js/lib/mp4/probe",
              "mux.js/lib/mp4/caption-parser",
              "@videojs/vhs-utils/dist/resolve-url",
              "@videojs/vhs-utils/dist/decode-b64-to-uint8-array"
            ],
            "assets": [
              "apps/octra/src/assets",
              "apps/octra/src/config",
              "apps/octra/src/media",
              "apps/octra/src/LICENSE.txt"
            ],
            "styles": [
              "node_modules/summernote/dist/summernote-bs4.css",
              "node_modules/video.js/dist/video-js.min.css",
              "apps/octra/src/styles.css"
            ],
            "scripts": [
              "apps/octra/src/assets/js/modernizr.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/arrive/minified/arrive.min.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap-material-design/dist/js/bootstrap-material-design.min.js",
              "node_modules/summernote/dist/summernote-bs4.min.js",
              "node_modules/video.js/dist/video.js",
              "node_modules/platform/platform.js",
              "node_modules/ajv/dist/ajv.min.js",
              "node_modules/jquery.caret/dist/jquery.caret.min.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/jszip/dist/jszip.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/octra/src/environments/environment.ts",
                  "with": "apps/octra/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "apps/octra/src/environments/environment.ts",
                  "with": "apps/octra/src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "octra:build",
            "port": 5321
          },
          "configurations": {
            "production": {
              "browserTarget": "octra:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "octra:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/octra/tsconfig.app.json",
              "apps/octra/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/octra/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/octra/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/octra"
          ]
        }
      }
    },
    "octra-e2e": {
      "root": "apps/octra-e2e",
      "sourceRoot": "apps/octra-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/octra-e2e/cypress.json",
            "tsConfig": "apps/octra-e2e/tsconfig.e2e.json",
            "devServerTarget": "octra:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "octra:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/octra-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/octra-e2e/**/*"
            ]
          }
        }
      }
    },
    "utilities": {
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utilities/tsconfig.lib.json",
              "libs/utilities/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/utilities/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/utilities"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/utilities",
            "tsConfig": "libs/utilities/tsconfig.lib.json",
            "packageJson": "libs/utilities/package.json",
            "main": "libs/utilities/src/index.ts",
            "assets": [
              "libs/utilities/*.md"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "media": {
      "root": "libs/media",
      "sourceRoot": "libs/media/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/media/tsconfig.lib.json",
              "libs/media/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/media/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/media/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/media"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/media",
            "tsConfig": "libs/media/tsconfig.lib.json",
            "packageJson": "libs/media/package.json",
            "main": "libs/media/src/index.ts",
            "assets": [
              "libs/media/*.md"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "annotation": {
      "root": "libs/annotation",
      "sourceRoot": "libs/annotation/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/annotation/tsconfig.lib.json",
              "libs/annotation/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/annotation/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/annotation/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/annotation"
          ]
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/annotation",
            "tsConfig": "libs/annotation/tsconfig.lib.json",
            "packageJson": "libs/annotation/package.json",
            "main": "libs/annotation/src/index.ts",
            "assets": [
              "libs/annotation/*.md"
            ]
          },
          "outputs": [
            "{options.outputPath}"
          ]
        }
      }
    },
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/components/tsconfig.lib.json",
            "project": "libs/components/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/components/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/components/tsconfig.lib.json",
              "libs/components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/components/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/components"
          ]
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "octra"
}