{
  "version": 1,
  "projects": {
    "octra": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/octra",
      "sourceRoot": "apps/octra/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/octra",
            "index": "apps/octra/src/index.html",

            "main": "apps/octra/src/main.ts",
            "polyfills": "apps/octra/src/polyfills.ts",
            "tsConfig": "apps/octra/tsconfig.app.json",
            "aot": true,
            "preserveSymlinks": true,
            "allowedCommonJsDependencies": [
              "js-sha256",
              "x2js",
              "konva",
              "crossevent",
              "contra/emitter",
              "flat",
              "dragula",
              "querystring",
              "crossvent"
            ],
            "assets": [
              "apps/octra/src/assets",
              "apps/octra/src/config",
              "apps/octra/src/media",
              "apps/octra/src/LICENSE.txt"
            ],
            "styles": [
              "node_modules/summernote/dist/summernote-bs4.css",
              "node_modules/video.js/dist/video-js.min.css",
              "apps/octra/src/styles.css"
            ],
            "scripts": [
              "apps/octra/src/assets/js/modernizr.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/arrive/minified/arrive.min.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap-material-design/dist/js/bootstrap-material-design.min.js",
              "node_modules/summernote/dist/summernote-bs4.min.js",
              "node_modules/video.js/dist/video.js",
              "node_modules/platform/platform.js",
              "node_modules/ajv/dist/ajv.min.js",
              "node_modules/jquery.caret/dist/jquery.caret.min.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/jszip/dist/jszip.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/octra/src/environments/environment.ts",
                  "with": "apps/octra/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "octra:build",
            "port": 5321
          },
          "configurations": {
            "production": {
              "browserTarget": "octra:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "octra:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/octra/tsconfig.app.json",
              "apps/octra/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/octra/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/octra/jest.config.js",
            "tsConfig": "apps/octra/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/octra/src/test-setup.ts"
          }
        }
      }
    },
    "octra-e2e": {
      "root": "apps/octra-e2e",
      "sourceRoot": "apps/octra-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/octra-e2e/cypress.json",
            "tsConfig": "apps/octra-e2e/tsconfig.e2e.json",
            "devServerTarget": "octra:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "octra:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/octra-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/octra-e2e/**/*"]
          }
        }
      }
    },
    "utilities": {
      "projectType": "library",
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/utilities/tsconfig.lib.json",
            "project": "libs/utilities/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/utilities/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utilities/tsconfig.lib.json",
              "libs/utilities/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utilities/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "tsConfig": "libs/utilities/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/utilities/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "annotation": {
      "projectType": "library",
      "root": "libs/annotation",
      "sourceRoot": "libs/annotation/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/annotation/tsconfig.lib.json",
            "project": "libs/annotation/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/annotation/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/annotation/tsconfig.lib.json",
              "libs/annotation/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/annotation/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/annotation/jest.config.js",
            "tsConfig": "libs/annotation/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/annotation/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "media": {
      "projectType": "library",
      "root": "libs/media",
      "sourceRoot": "libs/media/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/media/tsconfig.lib.json",
            "project": "libs/media/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/media/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/media/tsconf ig.lib.json",
              "libs/media/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/media/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/media/jest.config.js",
            "tsConfig": "libs/media/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/media/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/components/tsconfig.lib.json",
            "project": "libs/components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/components/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/components/tsconfig.lib.json",
              "libs/components/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/components/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "tsConfig": "libs/components/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/components/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "octra"
}
