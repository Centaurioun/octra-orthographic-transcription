{
  "version": 1,
  "projects": {
    "annotation": {
      "root": "libs/annotation",
      "sourceRoot": "libs/annotation/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/annotation/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/annotation"],
          "options": {
            "jestConfig": "libs/annotation/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/annotation",
            "tsConfig": "libs/annotation/tsconfig.lib.json",
            "packageJson": "libs/annotation/package.json",
            "main": "libs/annotation/src/index.ts",
            "assets": ["libs/annotation/*.md"]
          }
        }
      }
    },
    "components": {
      "projectType": "library",
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/components"],
          "options": {
            "project": "libs/components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components"],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/components/src/**/*.ts",
              "libs/components/src/**/*.html"
            ]
          }
        }
      }
    },
    "media": {
      "root": "libs/media",
      "sourceRoot": "libs/media/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/media/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/media"],
          "options": {
            "jestConfig": "libs/media/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/media",
            "tsConfig": "libs/media/tsconfig.lib.json",
            "packageJson": "libs/media/package.json",
            "main": "libs/media/src/index.ts",
            "assets": ["libs/media/*.md"]
          }
        }
      }
    },
    "octra": {
      "projectType": "application",
      "root": "apps/octra",
      "sourceRoot": "apps/octra/src",
      "prefix": "octra",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/octra",
            "index": "apps/octra/src/index.html",
            "main": "apps/octra/src/main.ts",
            "polyfills": "apps/octra/src/polyfills.ts",
            "tsConfig": "apps/octra/tsconfig.app.json",
            "allowedCommonJsDependencies": [
              "js-sha256",
              "jodit-angular",
              "x2js",
              "konva",
              "crossevent",
              "contra/emitter",
              "flat",
              "dragula",
              "platform",
              "global/document",
              "videojs",
              "ajv",
              "querystring",
              "crossvent",
              "xmldom",
              "videojs-vtt.js",
              "@videojs/xhr",
              "@babel/runtime/helpers/possibleConstructorReturn",
              "@babel/runtime/helpers/construct",
              "@babel/runtime/helpers/inherits",
              "mux.js/lib/tools/mp4-inspector",
              "mux.js/lib/tools/ts-inspector.js",
              "mux.js/lib/mp4/probe",
              "mux.js/lib/mp4/caption-parser",
              "@videojs/vhs-utils/dist/resolve-url",
              "@videojs/vhs-utils/dist/decode-b64-to-uint8-array"
            ],
            "assets": [
              "apps/octra/src/assets",
              "apps/octra/src/config",
              "apps/octra/src/media",
              "apps/octra/src/LICENSE.txt"
            ],
            "styles": [
              "node_modules/video.js/dist/video-js.min.css",
              "node_modules/mdb-ui-kit/css/mdb.min.css",
              "apps/octra/src/styles.css"
            ],
            "scripts": [
              "apps/octra/src/assets/js/modernizr.js",
              "node_modules/platform/platform.js",
              "node_modules/video.js/dist/video.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/jszip/dist/jszip.min.js"
            ],
            "extractLicenses": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/octra/src/environments/environment.ts",
                  "with": "apps/octra/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "octra:build:production",
              "port": 5321
            },
            "development": {
              "browserTarget": "octra:build:development",
              "port": 5321
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "octra:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/octra/src/**/*.ts",
              "apps/octra/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/octra"],
          "options": {
            "jestConfig": "apps/octra/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "octra-e2e": {
      "root": "apps/octra-e2e",
      "sourceRoot": "apps/octra-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/octra-e2e/cypress.json",
            "tsConfig": "apps/octra-e2e/tsconfig.e2e.json",
            "devServerTarget": "octra:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "octra:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/octra-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "utilities": {
      "root": "libs/utilities",
      "sourceRoot": "libs/utilities/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utilities/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utilities"],
          "options": {
            "jestConfig": "libs/utilities/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utilities",
            "tsConfig": "libs/utilities/tsconfig.lib.json",
            "packageJson": "libs/utilities/package.json",
            "main": "libs/utilities/src/index.ts",
            "assets": ["libs/utilities/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "octra"
}
